
class X{
	@Overide
	protected void finalize() throws Throwable {
		System.out.println("finalize() calling");
	}
}

public class GCDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("GC is executing...");
		for(int i=0; i<100000;i++) {
			new X();
		}
	}

}
