
public class BankApp {
	public static void main(String[] args) {
		BankAccount ba = new BankAccount();
		
		ba.setAccNo(100);
		ba.setName("Veena");
		ba.setBal(1000);
		
		System.out.println("Customer - " + ba.toString());
	}
}
