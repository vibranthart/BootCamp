
public class ExerciseOne {
	public static void main(String[] args) {
			
			for(int i=1; i<=19; i=i+2)
			{
				System.out.print(i+" ");
			}
			
			System.out.println("");
			
			for(int i=1; i<=19; i++)
			{
				System.out.print(i+" ");
			}
			System.out.println("");
			for(int i=1;i<5;i++)
			{
				for(int j=1; j<i;j++)
				{
					System.out.print(j+" ");
				}
				System.out.println("");
			}
		}
	}

