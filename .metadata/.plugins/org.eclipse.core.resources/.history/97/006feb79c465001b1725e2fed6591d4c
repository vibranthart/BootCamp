
public class BankApp {
	public static void main(String[] args) {
		SavingAc sa = new SavingAc(100,"Veena",10000,0.05);
		
		double amt = 6000;
		int result  = sa.withdraw(amt);
		
		if(result == 0)
		{
			System.out.println("Insufficient Balance");
			System.out.println("You want to withdraw - " + amt);
		}
		else if(result < 0)
		{
			System.out.println("Invalid Amount Entered");
		}
		else
		{
			System.out.println("Amount Withdrawn Successfully. Your Balance is : " + sa.getBal());
		}
	}
}

